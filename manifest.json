{
  "manifest_version": 2,

  "name": "Parents controll",
  "description": "This extension allow to block obscenities",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js":["functions.js"]
    }
  ],
  "web_accessible_resources": ["eng.json"]
}