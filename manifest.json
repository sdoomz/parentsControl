{
  "manifest_version": 2,

  "name": "Parents controll",
  "description": "This extension allow to block obscenities",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js":["functions.js"]
    }
  ]
}